package com.shopapp;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class AdminController {

	@GetMapping("")
	public String viewHomePage() {
		return "admin/index";
	}
	
	@GetMapping("/admin/test")
	public String test() {
		return "admin/index"
	}
}
